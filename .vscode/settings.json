        "statusBar.background": "#11285a",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#193b85",
        "statusBarItem.remoteBackground": "#11285a",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#11285a",
